{
  "name": "TrackIt",
  "manifest_version": 2,
  "description": "This extension is used to track user activities for design evaluation",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup" : "RegisterForm.html"
  },
 "icons": {
        "16" : "icon.png"
    },
  "background": {
        "scripts": ["bg.js"]
  },
  "permissions": [
        "tabs",
        "storage",
        "http://localhost/*"
  ],
  "options_page": "login4plugin.html"
}


